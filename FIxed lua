other:Button('Sword spin', function()
   while true do
local player = game.Players.LocalPlayer
local character = player.Character
local backpack = player.Backpack
local swords = {}
function look(dir)
    for i, v in pairs(dir:GetChildren()) do
        if(v:IsA("Tool"))then
            table.insert(swords,v)
        end
    end
end
look(backpack)
look(character)
    
for i, v in pairs(swords)do
    local sword             = v
    sword.Parent            = backpack
    sword.Handle.Massless   = true
    sword.Handle.CanCollide = true
    sword.Grip              = (sword.Grip*CFrame.Angles(1.50,0,0))*CFrame.new(0,0,0.5*(i-1))
    sword.Parent            = character
    sword.Handle.Touched:Connect(function(t)
        if(t.Parent)and(t.Parent:FindFirstChild("Humanoid"))and(t.Parent.Name~=player.Name)then
            sword.Handle.dmg.RemoteEvent:FireServer(t.Parent.Humanoid, math.huge)
        end
    end)
end
    wait(0.1)
end  
end)

other:Button('Sword ring', function()
           local player = game.Players.LocalPlayer
local distance = 50
local speed = 1
local mouse = player:GetMouse()
local rotating = true
local character = player.Character
local backpack = player.Backpack
local swords = {}
function look(dir)
    for i, v in pairs(dir:GetChildren()) do
        if(v:IsA("Tool"))then
            table.insert(swords,v)
        end
    end
end
look(backpack)
look(character)

mouse.KeyDown:Connect(function(k)
    if(k=="x")then
        rotating = false
    end
end)

for i, v in pairs(swords)do
    local sword = v
    sword.Parent = backpack
    sword.Handle.Massless = true
    sword.Handle.CanCollide = true
    sword.Grip = CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    
    sword.Grip = (sword.Grip*CFrame.Angles(0,(6.28/#swords)*i,0)*CFrame.new(distance*math.sin((6.28/#swords)*i),0,distance*math.cos((6.28/#swords)*i)))
    sword.Parent = character
end

local counter = 0
game:GetService("RunService").RenderStepped:Connect(function()
if(rotating)then
for i, v in pairs(swords)do
    counter = counter + 0.001*speed
    local sword = v
    sword.Parent = backpack
    sword.Handle.Massless = true
    sword.Handle.CanCollide = true
    sword.Grip = CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    local angle = ((6.28/#swords)*i)+counter
    sword.Grip = (sword.Grip*CFrame.Angles(0,angle,0)*CFrame.new(distance*math.sin(angle),0,distance*math.cos(angle)))
    sword.Parent = character
end
end
end)
end)
    
other:Button('Sword Form', function()
  local player = game.Players.LocalPlayer
local character = player.Character
local backpack = player.Backpack
local swords = {}
function look(dir)
    for i, v in pairs(dir:GetChildren()) do
        if(v:IsA("Tool"))then
            table.insert(swords,v)
        end
    end
end
look(backpack)
look(character)
    local oldswords = swords
    local newswords = {}
    for i = 1, 90 do
        newswords[i] = oldswords[i]
    end
    local swords = newswords
    local quotient = math.floor(#swords/6)
    local counter = 13
    local counter = 15
    for i = 1, 62 do
        local max = math.floor(#swords/6*i)+1
        local min = math.floor(#swords/62*(i-1))
        local diff = max-min
        for x = 1, math.sqrt(quotient)do
            for y = 1, math.sqrt(quotient)do
                local sword = swords[counter]
                sword.Parent = player.Backpack
                sword.Grip = CFrame.new(20,0,0)*CFrame.Angles(0,1.57,0)if(x==math.floor(math.sqrt(quotient)) and i<5)then
                    sword.Grip = sword.Grip*CFrame.Angles(40,-1.57/2,0)*CFrame.new(-1,0,-1)
                end
                sword.Grip = sword.Grip*CFrame.new(x*3.5,y*-3.5,8.75)*CFrame.new(-8.75,15,0)
                if(i==2)then
                    sword.Grip = sword.Grip*CFrame.Angles(0,1.57,0)
                elseif(i==3)then
                    sword.Grip = sword.Grip*CFrame.Angles(0,3.14,0)
                elseif(i==4)then
                    sword.Grip = sword.Grip*CFrame.Angles(0,3.14+1.57,0)
                elseif(i==5)then
                    sword.Grip = sword.Grip*CFrame.Angles(-1.57,0,0)*CFrame.new(-1.75,0,-1)
                elseif(i==6)then
                    sword.Grip = sword.Grip*CFrame.Angles(1.57,0,0)*CFrame.new(-1.75,2,1)
                end
                sword.Grip = sword.Grip*CFrame.new(1.7,-6.8 ,-2.3)
                sword.Parent = player.Character
                counter = counter + 1
            end
        end
    end  
end)

other:Button('Sword Shoulder', function()
  ---------------Non Fe By Alzn---------------

--YOU NEED 19 OR MORE SWORDS FOR THIS TO WORK!

---------------MAIN SCRIPT---------------
local LocalPlayer = game:GetService("Players").LocalPlayer
for index, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
if index == 1 then
tool.Grip = CFrame.new(-0.25, 0.256342292, 1.88046515, -4.2013224e-09, 1, -4.35090151e-08, 0.996938467, 7.5903932e-09, 0.0781891644, 0.078189142, -4.3047315e-08, -0.996938527)
elseif index == 2 then
